#version 460
in vec3 pos;
in vec3 uv;
in vec4 color;

out vec3 TexCoord;
out vec4 Spritecolor;


uniform mat4 projection;
uniform mat4 model;
uniform mat4 view;


void main()
{
	
	
	
    gl_Position = projection * view *vec4( pos.x ,pos.y, pos.z, 1.0);
    //gl_Position = projection*vec4( pos.x ,pos.y, pos.z, 1.0);
	TexCoord = uv;
	Spritecolor = color;
	
}